{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/product1/(.*)",
      "dest": "/product1/$1"
    },
    {
      "src": "/product2/(.*)",
      "dest": "/product2/$1"
    },
    {
      "src": "/product3/(.*)",
      "dest": "/product3/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/product1/(.*)",
      "destination": "/product1/$1"
    },
    {
      "source": "/product2/(.*)",
      "destination": "/product2/$1"
    },
    {
      "source": "/product3/(.*)",
      "destination": "/product3/$1"
    }
  ],
  "domains": [
    {
      "name": "example.com"
    },
    {
      "name": "product1.example.com",
      "type": "CNAME"
    },
    {
      "name": "product2.example.com",
      "type": "CNAME"
    }
  ],
  "alias": {
    "product1.example.com": "/product1",
    "product2.example.com": "/product2",
    "example.com": "/product3"
  }
}