{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "output"
      }
    }
  ],
  "routes": [
    {
      "src": "/express/(.*)",
      "dest": "/express/$1"
    },
    {
      "src": "/remix/(.*)",
      "dest": "/remix/$1"
    },
    {
      "src": "/react-spa/(.*)",
      "dest": "/react-spa/$1"
    }
  ],
  "domains": [
    {
      "name": "multi-vercel-pro-v2.vercel.app"
    },
    {
      "name": "express.multi-vercel-pro-v2.vercel.app",
      "type": "express"
    },
    {
      "name": "remix.multi-vercel-pro-v2.vercel.app",
      "type": "remix"
    },
    {
      "name": "react-spa.multi-vercel-pro-v2.vercel.app",
      "type": "react-spa"
    }
  ],
  "alias": {
    "multi-vercel-pro-v2.vercel.app": "/react-spa",
    "express.multi-vercel-pro-v2.vercel.app": "/express",
    "remix.multi-vercel-pro-v2.vercel.app": "/remix"
  }
}